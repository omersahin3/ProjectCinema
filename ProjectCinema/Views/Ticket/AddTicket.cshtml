@model ProjectCinema.Data.Models.Ticket
@{
    ViewData["Title"] = "AddTicket";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("AddTicket", "Ticket", FormMethod.Post))
{
    @Html.LabelFor(x => x.Session)
    @Html.DropDownListFor(x => x.SessionID, (List<SelectListItem>)ViewBag.v1, new { @class = "form-control " })
    <br />
    <button class="btn btn-info">SAVE</button>
}


